{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "2.1.15",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:network": "vite --host 0.0.0.0",
    "dev:https": "vite",
    "build": "node scripts/check-required-env.cjs && vite build",
    "build:prod": "vite build",
    "build:apk": "npm run build && npx cap sync android && cd android && gradlew.bat assembleDebug --no-daemon && cd .. && powershell -Command \"Copy-Item -Path 'android\\app\\build\\outputs\\apk\\debug\\*.apk' -Destination '.' -Force -Exclude 'output-metadata.json'\" && echo APK created in project root",
    "build:aab": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts\\build-aab.ps1",
    "build:aab:local": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts\\build-aab.ps1 -AllowLocalApiBaseUrl",
    "cap:init": "npx cap init",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "lint": "eslint .",
    "preview": "vite preview",

    "api:dev": "tsx watch api/server.ts",
    "api:Dev": "tsx watch api/server.ts",
    "api:start": "tsx api/server.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:appversion": "tsx scripts/upsert-appversions.ts"
  },
  "dependencies": {
      "@prisma/client": "^6.2.1",
    "@fastify/static": "^8.0.0",
    "@capacitor/filesystem": "^7.1.4",
    "@capacitor/geolocation": "^7.1.5",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/share": "^7.0.2",
    "@capacitor/status-bar": "^7.0.3",
    "@fastify/cors": "^10.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "fastify": "^5.2.1",
    "jsonwebtoken": "^9.0.2",
    "resend": "^4.0.1",
    "zod": "^3.24.1",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.4.7",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.3",
    "@eslint/js": "^9.9.1",
    "@types/file-saver": "^2.0.7",
    "@types/leaflet": "^1.9.20",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/xlsx": "^0.0.35",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.2",
    "prisma": "^6.2.1",
    "typescript": "5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
